      "source": [
        "# Demonstrates different data types in Python\n",
        "\n",
        "# Integer (whole number)\n",
        "age = 30\n",
        "print(f\"Age: {age}, Type: {type(age)}\")\n",
        "\n",
        "# Float (decimal number)\n",
        "price = 99.99\n",
        "print(f\"Price: {price}, Type: {type(price)}\")\n",
        "\n",
        "# String (text)\n",
        "name = \"Alice\"\n",
        "print(f\"Name: {name}, Type: {type(name)}\")\n",
        "\n",
        "# Boolean (True or False)\n",
        "is_student = True\n",
        "print(f\"Is Student: {is_student}, Type: {type(is_student)}\")\n",
        "\n",
        "# List (ordered collection of items)\n",
        "numbers = [1, 2, 3, 4, 5]\n",
        "print(f\"Numbers: {numbers}, Type: {type(numbers)}\")\n",
        "\n",
        "# Tuple (ordered, immutable collection of items)\n",
        "coordinates = (10, 20)\n",
        "print(f\"Coordinates: {coordinates}, Type: {type(coordinates)}\")\n",
        "\n",
        "# Dictionary (key-value pairs)\n",
        "person = {\"name\": \"Bob\", \"age\": 25, \"city\": \"New York\"}\n",
        "print(f\"Person: {person}, Type: {type(person)}\")\n",
        "\n",
        "# Set (unordered collection of unique items)\n",
        "unique_numbers = {1, 2, 2, 3, 3, 3}  # Duplicates are automatically removed\n",
        "print(f\"Unique Numbers: {unique_numbers}, Type: {type(unique_numbers)}\")\n",
        "\n",
        "# NoneType (represents the absence of a value)\n",
        "nothing = None\n",
        "print(f\"Nothing: {nothing}, Type: {type(nothing)}\")\n",
        "\n",
        "\n",
        "# Demonstrating some basic operations\n",
        "\n",
        "# Integer arithmetic\n",
        "x = 10\n",
        "y = 5\n",
        "sum_result = x + y\n",
        "difference_result = x - y\n",
        "product_result = x * y\n",
        "division_result = x / y\n",
        "print(f\"Sum: {sum_result}\")\n",
        "print(f\"Difference: {difference_result}\")\n",
        "print(f\"Product: {product_result}\")\n",
        "print(f\"Division: {division_result}\")\n",
        "\n",
        "\n",
        "# String concatenation\n",
        "first_name = \"Charlie\"\n",
        "last_name = \"Brown\"\n",
        "full_name = first_name + \" \" + last_name  # Notice the space for readability\n",
        "print(f\"Full Name: {full_name}\")\n",
        "\n",
        "# Boolean logic\n",
        "is_adult = age >= 18  # Assuming 'age' is already defined\n",
        "print(f\"Is Adult: {is_adult}\")\n",
        "\n",
        "# List access and modification\n",
        "print(f\"First number in the list: {numbers[0]}\")  # Accessing the first element (index 0)\n",
        "numbers[2] = 100 #Modifying the third element (index 2)\n",
        "print(f\"Modified list : {numbers}\")"
      ],
